{
  "name": "ServidorNodeForAngular",
  "version": "2.0.0",
  "author": "Starley Cazorla",
  "description": "Servidor NodeJs para aplicações angular com banco de dados sqlite",
  "private": true,
  "main": "./src/bin/server.js",
  "bin": "./src/bin/server.js",
  "scripts": {
    "start": "node ./src/bin/server.js prod",
    "nodemon": "nodemon ./src/bin/server.js dev",
    "build": "pkg ."
  },
  "dependencies": {
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "http": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "node-unzip-2": "^0.2.8",
    "sqlite3": "^5.0.8",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.18",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "*/**",
    "outputPath": "dist"
  }
}
